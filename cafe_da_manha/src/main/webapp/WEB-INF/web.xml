<?xml version="1.0" encoding="UTF-8"?>
<web-app 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
	id="WebApp_ID" version="3.1"
>
 <display-name>cafe_da_manha</display-name>
  
	<servlet>
		<servlet-name>cadastrar</servlet-name>
		<servlet-class>cadastraFuncionario.cadastrarFuncionarioController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cadastrar</servlet-name>
		<url-pattern>/cadastraFuncionario</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>buscar</servlet-name>
		<servlet-class>controller.buscaFuncionario</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>buscar</servlet-name>
		<url-pattern>/buscaFuncionario</url-pattern>
	</servlet-mapping>
</web-app>